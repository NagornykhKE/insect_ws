<launch>
  <include
    file="$(find gazebo_ros)/launch/empty_world.launch" >
    <arg name="paused" value="true"/>
  </include>  
   
   <param name="robot_description" command="xacro --inorder '$(find old)/urdf/old.urdf'"/>
  <node
    name="spawn_model"
    pkg="gazebo_ros"
    type="spawn_model"
    args="-urdf -param robot_description -model old"
    output="screen" />

    
    
</launch>
